{
    "cSpell.words": [
        "dotenv",
        "helpfull",
        "powerplatfrom",
        "pydantic"
    ],
    "cSpell.ignorePaths": [
        "**/*.jsonl",
        "**/.venv/**",
        "**/__pycache__/**",
        "experiments/runs/**",
        "**/.mypy_cache/**",
        "**/*.lock",
        ""
    ],
    "search.exclude": {
        "experiments/runs/**": true
    },
    "files.watcherExclude": {
        "experiments/runs/**": true
    },
    "python.analysis.exclude": [
        "!experiments/**"
    ],
    "python.analysis.extraPaths": [
        "./src"
    ]
}